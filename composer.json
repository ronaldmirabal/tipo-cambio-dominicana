{
    "name": "ronaldmirabal/tipo-cambio-dominicana",
    "description": "TipoCambioDomincana es un paquete desarrollado para facilitar la consulta del tipo de cambio oficial del DÃ³lar (USD) y del Euro (EUR) frente al Peso Dominicano (DOP).",
    "keywords": [
        "ronaldmirabal",
        "laravel",
        "tipo-cambio-dominicana"
    ],
    "homepage": "https://github.com/ronaldmirabal/tipo-cambio-dominicana",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronald Mirabal Gomez",
            "email": "ronaldmirabal@outlook.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.16",
        "symfony/dom-crawler": "^7.3"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ronaldmirabal\\TipoCambioDominicana\\": "src/",
            "Ronaldmirabal\\TipoCambioDominicana\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ronaldmirabal\\TipoCambioDominicana\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ronaldmirabal\\TipoCambioDominicana\\TipoCambioDominicanaServiceProvider"
            ],
            "aliases": {
                "TipoCambioDominicana": "Ronaldmirabal\\TipoCambioDominicana\\Facades\\TipoCambioDominicana"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
